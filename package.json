{
  "name": "@adexchange/aeg-mysql",
  "version": "0.0.15",
  "description": "Mysql driver",
  "main": "./lib/extensions.js",
  "author": "Justin Durham",
  "repository": {
    "type": "git",
    "url": "git://git@bitbucket.org:AdExchangeGrp/aeg-mysql.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "test": "mocha --recursive test/",
    "test:watch": "npm run test -- -w",
    "test:coverage": "istanbul cover _mocha -- --recursive test/",
    "jshint": "jshint .",
    "jshint:watch": "npm run jshint && onchange \"**/*.js\" \".jshint*\" -- npm run jshint",
    "dev": "parallelshell \"npm run jshint:watch\" \"npm run test:watch\"",
    "build": "npm run jshint && mocha --recursive --reporter mocha-junit-reporter test/unit/",
    "postversion": "git push && git push --tags && npm publish ."
  },
  "devDependencies": {
    "istanbul": "^0.3.17",
    "mocha": "^2.2.5",
    "mocha-junit-reporter": "^1.9.1",
    "onchange": "^1.1.0",
    "parallelshell": "^1.1.1",
    "should": "^5.2.0"
  },
  "dependencies": {
    "@adexchange/aeg-logger": "^0.0.23",
    "async": "^0.9.0",
    "config": "^1.12.0",
    "mysql": "^2.5.5",
    "underscore": "^1.8.2",
    "terminus": "^1.0.12"
  },
  "engine": "node >= 4.1.0"
}
